{
  "name": "@fisand/webext-template",
  "version": "0.2.0",
  "description": "webext template powered by vite and react",
  "keywords": [
    "react",
    "vite",
    "extension",
    "chrome",
    "firefox",
    "template"
  ],
  "license": "MIT",
  "packageManager": "pnpm@7.23.0",
  "homepage": "https://github.com/fisand/webext-dapp-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/fisand/webext-dapp-template"
  },
  "bugs": {
    "url": "https://github.com/fisand/webext-dapp-template/issues",
    "email": "zouhangsweet@gmail.com"
  },
  "author": "zouhang",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:bg": "tsup --watch ./src",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:*",
    "build:web": "vite build",
    "build:prepare": "esno scripts/prepare.ts",
    "build:js": "vite build --config vite.config.content.ts",
    "build:bg": "tsup --config tsup.config.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "ci:publish": "pnpm changeset publish",
    "ci:version": "pnpm changeset version"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@metamask/providers": "^10.2.1",
    "@unocss/reset": "^0.50.6",
    "buffer": "^6.0.3",
    "document-ready": "^2.0.2",
    "events": "^3.3.0",
    "metamask-extension-provider": "^3.0.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.12.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.17.0",
    "@ffflorian/jszip-cli": "^3.1.5",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/document-ready": "^2.0.0",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^2.0.1",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "esno": "^0.9.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.0",
    "kolorist": "^1.5.0",
    "lint-staged": "^11.1.2",
    "node-stdlib-browser": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6",
    "rimraf": "^3.0.2",
    "terser": "^5.16.6",
    "tsup": "^6.6.3",
    "typescript": "4.9.3",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "^0.4.5",
    "vite": "^3.2.4",
    "vite-plugin-checker": "~0.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-node-stdlib-browser": "^0.2.1",
    "web-ext": "^6.3.0",
    "webext-bridge": "^4.1.1",
    "webextension-polyfill": "^0.8.0"
  }
}
